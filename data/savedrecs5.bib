@article{ ISI:000291402500003,
Author = {Woinarski, John C. Z. and Legge, Sarah and Fitzsimons, James A. and
   Traill, Barry J. and Burbidge, Andrew A. and Fisher, Alaric and Firth,
   Ron S. C. and Gordon, Iain J. and Griffiths, Anthony D. and Johnson,
   Christopher N. and McKenzie, Norm L. and Palmer, Carol and Radford, Ian
   and Rankmore, Brooke and Ritchie, Euan G. and Ward, Simon and Ziembicki,
   Mark},
Title = {{The disappearing mammal fauna of northern Australia: context, cause, and
   response}},
