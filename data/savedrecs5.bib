@article{ ISI:000233992100004,
Author = {Burdakov, D and Luckman, SM and Verkhratsky, A},
Title = {{Glucose-sensing neurons of the hypothalamus}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2005}},
