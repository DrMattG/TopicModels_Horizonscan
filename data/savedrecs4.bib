@article{ ISI:000263389100002,
Author = {Tin, T. and Fleming, Z. L. and Hughes, K. A. and Ainley, D. G. and
   Convey, P. and Moreno, C. A. and Pfeiffer, S. and Scott, J. and Snape,
   I.},
Title = {{Impacts of local human activities on the Antarctic environment}},
