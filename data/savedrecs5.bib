@article{ ISI:000305262500006,
Author = {Stephens, Scott L. and McIver, James D. and Boerner, Ralph E. J. and
   Fettig, Christopher J. and Fontaine, Joseph B. and Hartsough, Bruce R.
   and Kennedy, Patricia L. and Schwilk, Dylan W.},
Title = {{The Effects of Forest Fuel-Reduction Treatments in the United States}},
Journal = {{BIOSCIENCE}},
Year = {{2012}},
Volume = {{62}},
Number = {{6}},
Pages = {{549-560}},
Month = {{JUN}},
Abstract = {{The current conditions of many seasonally dry forests in the western and
   southern United States, especially those that once experienced low- to
   moderate-intensity fire regimes, leave them uncharacteristically
   susceptible to high-severity wildfire. Both prescribed fire and its
   mechanical surrogates are generally successful in meeting short-term
   fuel-reduction objectives such that treated stands are more resilient to
   high-intensity wildfire. Most available evidence suggests that these
   objectives are typically accomplished with few unintended consequences,
   since most ecosystem components (vegetation, soils, wildlife, bark
   beetles, carbon sequestration) exhibit very subtle effects or so
   measurable effects at all. Although mechanical treatments do not serve
   as complete surrogates for fire, their application can help mitigate
   costs and liability in some areas. Desired treatment effects on fire
   hazards are transient, which indicates that after fuel-reduction
   management start:;, managers need to be persistent with repeated
   treatment, especially in the faster-growing forests in the southern
   United States.}},
