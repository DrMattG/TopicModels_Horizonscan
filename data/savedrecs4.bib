@article{ ISI:000255503300012,
Author = {Cardillo, Marcel and Mace, Georgina M. and Gittleman, John L. and Jones,
   Kate E. and Bielby, Jon and Purvis, Andy},
Title = {{The predictability of extinction: biological and external correlates of
   decline in mammals}},
