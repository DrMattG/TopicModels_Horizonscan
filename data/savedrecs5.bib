@article{ ISI:000175806000007,
Author = {McDonnell, MD and Possingham, HP and Ball, IR and Cousins, EA},
Title = {{Mathematical methods for spatially cohesive reserve design}},
Journal = {{ENVIRONMENTAL MODELING \& ASSESSMENT}},
Year = {{2002}},
Volume = {{7}},
Number = {{2, SI}},
Pages = {{107-114}},
Month = {{JUN}},
Abstract = {{The problem of designing spatially cohesive nature reserve systems that
   meet biodiversity objectives is formulated as a nonlinear integer
   programming problem. The multiobjective function minimises a combination
   of boundary length, area and failed representation of the biological
   attributes we are trying to conserve. The task is to reserve a subset of
   sites that best meet this objective. We use data on the distribution of
   habitats in the Northern Territory, Australia, to show how simulated
   annealing and a greedy heuristic algorithm can be used to generate good
   solutions to such large reserve design problems, and to compare the
   effectiveness of these methods.}},
