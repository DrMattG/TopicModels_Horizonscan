@article{ ISI:000084684400002,
Author = {Safford, HD},
Title = {{Brazilian Paramos I. An introduction to the physical environment and
   vegetation of the campos de altitude}},
Journal = {{JOURNAL OF BIOGEOGRAPHY}},
