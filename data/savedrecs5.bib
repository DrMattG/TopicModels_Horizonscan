@article{ ISI:000283448800004,
Author = {La Sorte, Frank A. and Jetz, Walter},
Title = {{Projected range contractions of montane biodiversity under global
   warming}},
Journal = {{PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2010}},
