@article{ ISI:A1997YD79000012,
Author = {Sisk, TD and Haddad, NM and Ehrlich, PR},
Title = {{Bird assemblages in patchy woodlands: Modeling the effects of edge and
   matrix habitats}},
Journal = {{ECOLOGICAL APPLICATIONS}},
Year = {{1997}},
Volume = {{7}},
