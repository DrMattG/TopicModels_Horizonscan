@article{ ISI:000227249400001,
Author = {Schlaepfer, MA and Sherman, PW and Blossey, B and Runge, MC},
Title = {{Introduced species as evolutionary traps}},
Journal = {{ECOLOGY LETTERS}},
Year = {{2005}},
Volume = {{8}},
Number = {{3}},
Pages = {{241-246}},
Month = {{MAR}},
Abstract = {{Invasive species can alter environments in such a way that normal
   behavioural decision-making rules of native species are no longer
   adaptive. The evolutionary trap concept provides a useful framework for
   predicting and managing the impact of harmful invasive species. We
   discuss how native species can respond to changes in their selective
   regime via evolution or learning. We also propose novel management
   strategies to promote the long-term co-existence of native and
   introduced species in cases where the eradication of the latter is
   either economically or biologically unrealistic.}},
