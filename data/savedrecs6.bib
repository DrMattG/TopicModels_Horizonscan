@article{ ISI:000260252000026,
Author = {Gibbons, P. and Lindenmayer, D. B. and Fischer, J. and Manning, A. D.
   and Weinberg, A. and Seddon, J. and Ryan, P. and Barrett, G.},
Title = {{The Future of Scattered Trees in Agricultural Landscapes}},
Journal = {{CONSERVATION BIOLOGY}},
