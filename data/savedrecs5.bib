@article{ ISI:000168502300004,
Author = {Khurana, E and Singh, JS},
Title = {{Ecology of seed and seedling growth for conservation and restoration of
   tropical dry forest : a review}},
Journal = {{ENVIRONMENTAL CONSERVATION}},
