@article{ ISI:000251210800009,
Author = {Bowen, Michiala E. and McAlpine, Clive A. and House, Alan P. N. and
   Smith, Geoffrey C.},
Title = {{Regrowth forests on abandoned agricultural land: A review of their
   habitat values for recovering forest fauna}},
