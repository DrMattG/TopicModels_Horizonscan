@article{ ISI:A1997YJ88300008,
Author = {Wasser, SK and Houston, CS and Koehler, GM and Cadd, GG and Fain, SR},
Title = {{Techniques for application of faecal DNA methods to field studies of
   Ursids}},
Journal = {{MOLECULAR ECOLOGY}},
