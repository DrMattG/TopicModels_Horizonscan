@article{ ISI:000256151100007,
Author = {Claassen, Roger and Cattaneo, Andrea and Johansson, Robert},
Title = {{Cost-effective design of agri-environmental payment programs: US
   experience in theory and practice}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{2008}},
Volume = {{65}},
