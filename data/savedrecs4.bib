@article{ ISI:000087400400009,
Author = {Ranius, T and Jansson, N},
Title = {{The influence of forest regrowth, original canopy cover and tree size on
   saproxylic beetles associated with old oaks}},
Journal = {{BIOLOGICAL CONSERVATION}},
Year = {{2000}},
Volume = {{95}},
Number = {{1}},
Pages = {{85-94}},
Month = {{AUG}},
Abstract = {{Abandoned management has caused many sites with free-standing, large
   oaks (Quercus robur) to become more shaded. This study shows how forest
   regrowth affects beetle species associated with old oaks in
   south-eastern Sweden. Beetles were trapped by pitfall traps placed in
   hollows and window traps placed near hollows in oak trunks in pasture
   woodlands. We assessed the influence of forest regrowth, tree size and
   original canopy cover on the species richness of saproxylic beetles (a
   total of 120 species identified) and the occurrence of 68 saproxylic
   beetle species in particular. Species richness was greatest in stands
   with large, free-standing trees. Large girth as well as low canopy cover
   increased frequency of occurrence for several species. Forest regrowth
   was found to be detrimental for many beetle species. As most localities
   with endangered beetles living in old oaks are small and isolated,
   ongoing management and the restoration of abandoned pasture woodlands
   should have a high priority in nature conservation. (C) 2000 Elsevier
   Science Ltd. All rights reserved.}},
